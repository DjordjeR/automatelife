{
  "$schema": ".\\schema.json",
  "name": "python",
  "structure": [
    {
      "dir": null,
      "files": ["setup.py", ".gitignore", "README.md"]
    },
    {
      "dir":  ["$projectName"],
      "files": ["__init__.py", "core.py"]
    },
    {
      "dir": ["bin"],
      "files": ["example.py"]
    },
    {
      "dir": ["tests"]
    },
    {
      "dir": ["docs"]
    }
  ],
  "gitignore": [
    "python",
    "visualstudiocode",
    "pycharm",
    "venv"
  ],
  "commands": [
    "git init"
  ]
}